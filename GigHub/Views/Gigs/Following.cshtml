@model IEnumerable<string>
@{
    ViewBag.Title = "Following";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Artists I am Following</h2>
<div class="followee">
    @if (Model.Count() == 0)
    {
        <p>There is no following artists.</p>
    }
    else
    {
        foreach (var userName in Model)
        {
            <div class="artist-name">@userName</div>
        }
    }

</div>


